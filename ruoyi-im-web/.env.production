
# 生产环境配置
ENV = 'production'

# 若依管理系统/生产环境
VUE_APP_BASE_API = '/prod-api'

# WebSocket服务器地址
VUE_APP_WS_API = 'ws://172.168.20.73:9090'

# 路由基础路径
VUE_APP_CONTEXT_PATH = '/'

# 是否启用Mock数据
VUE_APP_USE_MOCK = false

# 是否显示设置的底部信息
VUE_APP_SHOW_SETTINGS = false

# 是否显示 tagsView
VUE_APP_SHOW_TAGS_VIEW = true

# 是否固定头部
VUE_APP_FIXED_HEADER = true

# 是否显示Logo
VUE_APP_SHOW_LOGO = true

# 是否显示动态标题
VUE_APP_DYNAMIC_TITLE = true

# 是否启用WebRTC功能
VUE_APP_ENABLE_WEBRTC = true

# WebRTC ICE服务器配置（生产环境应使用自己的STUN/TURN服务器）
VUE_APP_ICE_SERVERS = '[{"urls":"stun:stun.your-domain.com:3478"},{"urls":"turn:turn.your-domain.com:3478","username":"your-username","credential":"your-password"}]'

# 文件上传配置
VUE_APP_MAX_FILE_SIZE = 100 * 1024 * 1024
VUE_APP_ALLOWED_FILE_TYPES = '.jpg,.jpeg,.png,.gif,.doc,.docx,.xls,.xlsx,.pdf,.zip,.rar'

# 消息配置
VUE_APP_MAX_MESSAGE_LENGTH = 1000
VUE_APP_MESSAGE_PAGE_SIZE = 20

# 是否启用消息加密
VUE_APP_ENABLE_MESSAGE_ENCRYPTION = true