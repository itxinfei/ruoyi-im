# 开发环境配置
NODE_ENV = 'development'

# API基础路径
VUE_APP_BASE_API = 'http://localhost:8081/im'

# WebSocket服务器地址
VUE_APP_WS_API = 'ws://localhost:8081/im/ws'

# 路由基础路径
VUE_APP_CONTEXT_PATH = '/'

# 是否启用Mock数据
VUE_APP_USE_MOCK = true

# 标题
VUE_APP_TITLE = 'RuoYi-IM'

# 端口
PORT = 8080

# 是否开启gzip压缩
VUE_APP_COMPRESSION = true

# 是否开启错误日志
VUE_APP_ERROR_LOG = true

# 是否开启性能监控
VUE_APP_PERFORMANCE = true

# 是否开启PWA
VUE_APP_PWA = false

# 是否开启多语言
VUE_APP_I18N = true

# 默认语言
VUE_APP_DEFAULT_LANG = 'zh-CN'