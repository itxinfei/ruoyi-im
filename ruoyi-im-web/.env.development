# 开发环境配置
NODE_ENV = 'development'

# API基础路径
VITE_BASE_API = 'http://localhost:8080/api/im'

# WebSocket服务器地址
# 注意：后端WebSocket端点是 /ws/im，不是 /im/ws
VITE_APP_WS_API = 'localhost:8080'

# 路由基础路径
VITE_CONTEXT_PATH = '/'

# 是否启用Mock数据
VITE_USE_MOCK = true

# 标题
VITE_TITLE = 'RuoYi-IM'

# 端口
PORT = 8080

# 是否开启gzip压缩
VITE_COMPRESSION = true

# 是否开启错误日志
VITE_ERROR_LOG = true

# 是否开启性能监控
VITE_PERFORMANCE = true

# 是否开启PWA
VITE_PWA = false

# 是否开启多语言
VITE_I18N = true

# 默认语言
VITE_DEFAULT_LANG = 'zh-CN'