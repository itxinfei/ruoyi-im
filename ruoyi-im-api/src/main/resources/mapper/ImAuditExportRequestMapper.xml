<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.im.mapper.ImAuditExportRequestMapper">

    <resultMap type="com.ruoyi.im.domain.ImAuditExportRequest" id="ImAuditExportRequestResult">
        <id property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="targetType" column="target_type"/>
        <result property="targetId" column="target_id"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="status" column="status"/>
        <result property="filePath" column="file_path"/>
        <result property="errorMessage" column="error_message"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectImAuditExportRequestVo">
        select id, user_id, target_type, target_id, start_time, end_time, status, file_path, error_message, create_time, update_time 
        from im_audit_export_request
    </sql>

    <select id="selectImAuditExportRequestList" parameterType="com.ruoyi.im.domain.ImAuditExportRequest" resultMap="ImAuditExportRequestResult">
        <include refid="selectImAuditExportRequestVo"/>
        <where>  
            <if test="userId != null"> AND user_id = #{userId}</if>
            <if test="targetType != null and targetType != ''"> AND target_type = #{targetType}</if>
            <if test="targetId != null"> AND target_id = #{targetId}</if>
            <if test="status != null and status != ''"> AND status = #{status}</if>
        </where>
        order by create_time desc
    </select>

    <select id="selectImAuditExportRequestById" parameterType="Long" resultMap="ImAuditExportRequestResult">
        <include refid="selectImAuditExportRequestVo"/>
        where id = #{id}
    </select>

    <insert id="insertImAuditExportRequest" parameterType="com.ruoyi.im.domain.ImAuditExportRequest" useGeneratedKeys="true" keyProperty="id">
        insert into im_audit_export_request
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="targetType != null">target_type,</if>
            <if test="targetId != null">target_id,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="status != null">status,</if>
            <if test="filePath != null">file_path,</if>
            <if test="errorMessage != null">error_message,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="targetType != null">#{targetType},</if>
            <if test="targetId != null">#{targetId},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="status != null">#{status},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="errorMessage != null">#{errorMessage},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateImAuditExportRequest" parameterType="com.ruoyi.im.domain.ImAuditExportRequest">
        update im_audit_export_request
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="targetType != null">target_type = #{targetType},</if>
            <if test="targetId != null">target_id = #{targetId},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="errorMessage != null">error_message = #{errorMessage},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteImAuditExportRequestById" parameterType="Long">
        delete from im_audit_export_request where id = #{id}
    </delete>

    <delete id="deleteImAuditExportRequestByIds" parameterType="String">
        delete from im_audit_export_request where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
